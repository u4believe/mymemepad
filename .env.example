# Private Key for Deployment
# Get this from your wallet (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# Network Configuration
NETWORK_URL=https://testnet.rpc.intuition.systems
CHAIN_ID=13579

# Contract Addresses (deploy these first or use existing ones)
TRUST_TOKEN_ADDRESS=0x... # Address of deployed $TRUST token
TREASURY_ADDRESS=0x... # Your treasury address for fees
UNISWAP_FACTORY_ADDRESS=0x... # Optional: UniswapV2 factory address

# Token Creation Parameters (for scripts/createToken.js)
TOKEN_NAME=My Meme Token
TOKEN_SYMBOL=MMT
MAX_SUPPLY=1000000000000000000000000 # 1M tokens in wei

# Optional: API Keys for verification
ETHERSCAN_API_KEY=your_etherscan_api_key
INTUITION_API_KEY=your_intuition_api_key

# Foundry Configuration (for script/Deploy.s.sol)
FOUNDRY_PROFILE=intuition
FOUNDRY_MNEMONIC=your_mnemonic_here
FOUNDRY_ETHERSCAN_API_KEY=your_etherscan_api_key